{
  "extends": ["airbnb", "airbnb/hooks", "plugin:jest/recommended", "prettier", "prettier/babel", "prettier/react"],
  "parser": "babel-eslint",
  "plugins": ["babel", "import", "jest", "jsx-a11y", "prettier", "react"],
  "rules": {
    "prettier/prettier": ["error"]
  },
  "env": {
    "jest/globals": true,
    "browser": true,
    "node": true,
    "mongo": true,
    "es6": true
  }
}
